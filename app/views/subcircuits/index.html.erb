<div>
  <div>
    <h1>
      Subcircuit List
    </h1>
  </div>
</div>

<hr>

<hr>
<div>
  <div>
    <table border="1">
      <tr>
        <th>
          Subcircuit Number
        </th>

        <th>
        </th>
      </tr>

      <% @list_of_subcircuits.each do |a_subcircuit| %>
      <tr>

        <td>
          Subcircuit <%= a_subcircuit.number %>
        </td>

        <td>
          <a href="/subcircuits/<%= a_subcircuit.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
